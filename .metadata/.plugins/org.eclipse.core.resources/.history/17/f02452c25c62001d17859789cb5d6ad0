package com.comparatorcomparable;

import java.util.ArrayList;
import java.util.Collections;

public class SortEmployeeByEid {

	public static void main(String[] args) {
		
		ArrayList<Employee> list = new ArrayList<>();
		
		Employee e1 = new Employee(11, "Dev", 10, "Active", 29899);
		Employee e2 = new Employee(13, "Nikhil", 20, "Passive", 19899);
		Employee e3 = new Employee(12, "Karina", 10, "Passive", 49899);
		Employee e4 = new Employee(19, "Flavio", 20, "Active", 39499);
		Employee e5 = new Employee(16, "Kumar", 30, "Active", 31899);
		
		list.add(e1);list.add(e2);list.add(e3);list.add(e4);list.add(e5);
		
		System.out.println(list);
		
		System.out.println("Sorted Employee based on EID");
		
		Collections.sort(list, new SortEmployeeByEidAndNameComparator());
		System.out.println(list);
		
	}

}
