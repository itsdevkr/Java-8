package com.arrays;

public class CheckDuplicate {

	static boolean checkDup(int [] arr, int n) {
		boolean flag = false;
		for(int i=0; i<n; i++) {
			int element = arr[i];
			while(i<n) {
				if(arr[i+1]==element) {
					flag = true;
					break;
				}
			}
		}
		return flag;
	}
	public static void main(String[] args) {
		int [] arr = {1,2,3,1};
		System.out.println(checkDup(arr, arr.length));

	}

}
