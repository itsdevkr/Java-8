package com.searchingsortings;

import java.util.ArrayList;
import java.util.List;

public class FirstAndLastOccurance {

	 static ArrayList<Long> find(long arr[], int n, int x)
	    {
	        List<Long> list = new ArrayList<Long>();
	        list.add(new Long(list.indexOf(new Long(x))));
	        list.add(new Long(list.lastIndexOf(new Long(x))));
	        
	        return (ArrayList<Long>) list;
	    }
	public static void main(String[] args) {
		long[] arr = {1, 3, 5, 5, 5, 5, 67, 123, 125};
		int n = arr.length;
		int x = 5;

	}

}
